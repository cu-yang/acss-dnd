{
  "name": "@acss-dnd/template",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "bugs": {
    "url": "https://github.com/cu-yang/acss-dnd/issues"
  },
  "homepage": "https://github.com/cu-yang/acss-dnd/packages/template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cu-yang/acss-dnd.git"
  },
  "author": "cuyang",
  "license": "MIT",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && yarn tsc",
    "tsc": "vue-tsc --declaration --emitDeclarationOnly --outfile ./dist/index.d.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.0",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {
    "daisyui": "^2.13.6",
    "vite": "^2.9.1",
    "vue": "^3.2.31"
  }
